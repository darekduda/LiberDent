            </div>
            <div class="form-group">
                <label class="title" asp-for="Adres.NumerLokalu"></label>
                <input asp-for="Adres.NumerLokalu" class="inputStyle" />
                <span asp-validation-for="Adres.NumerLokalu" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="title" asp-for="Adres.KodPocztowy"></label>
                <input asp-for="Adres.KodPocztowy" class="inputStyle" />
                <span asp-validation-for="Adres.KodPocztowy" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="title" asp-for="Adres.Poczta"></label>
                <input asp-for="Adres.Poczta" class="inputStyle" />
                <span asp-validation-for="Adres.Poczta" class="text-danger"></span>
            </div>
            <button type="submit" class="btn-flat light-blue lighten-1">Zarejestruj</button>
        </form>
    </div>
</div>
@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}
